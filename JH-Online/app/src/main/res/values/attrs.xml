<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="home_radio_button">
        <attr name="button_title" format="string" />
        <attr name="image_normal" format="reference" />
        <attr name="image_press" format="reference" />
        <attr name="color_normal" format="reference" />
        <attr name="color_press" format="reference" />
        <attr name="figure" format="string" />
        <attr name="showCicle" format="boolean" />
        <attr name="checked" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ProjectToolbar">
        <attr name="toolbar_title" format="string" />
        <attr name="toolbar_right" format="string" />
        <attr name="toolbar_right_mode" format="integer">
            <flag name="none" value="0" />
            <flag name="black" value="1" />
            <flag name="blue" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <!--星星间距-->
        <attr name="starDistance" format="dimension" />
        <!--星星大小-->
        <attr name="starSize" format="dimension" />
        <!--星星个数-->
        <attr name="starCount" format="integer" />
        <!--星星空图-->
        <attr name="starEmpty" format="reference" />
        <!--星星满图-->
        <attr name="starFill" format="reference" />
    </declare-styleable>
    <declare-styleable name="menu_button">
        <attr name="menu_button_title" format="string" />
        <attr name="menu_button_checked" format="boolean" />
        <attr name="menu_button_image_normal" format="reference" />
        <attr name="menu_button_image_press" format="reference" />
        <attr name="menu_button_color_normal" format="reference" />
        <attr name="menu_button_color_press" format="reference" />
        <attr name="menu_button_resource" format="reference" />
    </declare-styleable>
</resources>